#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@microsoft/rush-stack-compiler-2.9/bin/rush-api-extractor" "$@"
  ret=$?
else 
  node  "$basedir/../@microsoft/rush-stack-compiler-2.9/bin/rush-api-extractor" "$@"
  ret=$?
fi
exit $ret
